{
  "name": "turborepo-summary-analyzer",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for turborepo-summary-analyzer goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "vite build --mode test && ember test --path dist"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/runtime": "^7.28.4",
    "@ember/test-helpers": "^5.2.2",
    "@embroider/core": "^4.1.3",
    "@embroider/macros": "^1.18.1",
    "@embroider/vite": "^1.2.0",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "alpha",
    "@glint/template": "alpha",
    "@glint/tsserver-plugin": "alpha",
    "@rollup/plugin-babel": "^6.0.4",
    "@tsconfig/ember": "^3.0.10",
    "@types/d3": "^7.4.3",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-eslint": "^0.5.0",
    "ember-hbs-minifier": "^1.3.0",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.3",
    "ember-source": "6.8.0-alpha.7",
    "ember-strict-application-resolver": "^0.1.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.35.0",
    "prettier": "3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.5.0",
    "typescript": "~5.9.2",
    "vite": "npm:rolldown-vite@latest"
  },
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">= 24"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "imports": {
    "#file": "./app/file.ts"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "dependencies": {
    "@ember/test-waiters": "^4.1.1",
    "@observablehq/plot": "^0.6.16",
    "nvp.ui": "^0.4.0",
    "d3": "^7.9.0",
    "ember-element-helper": "^0.8.8",
    "ember-primitives": "^0.40.0",
    "ember-resources": "^7.0.7",
    "idb": "8.0.3"
  },
  "pnpm": {
    "overrides": {
      "@glimmer/component": "^2.0.0",
      "@glint/core": "$@glint/core",
      "@glint/template": "$@glint/template"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  }
}
